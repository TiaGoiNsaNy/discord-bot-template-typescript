{
  "name": "discord-bot-template-ts",
  "version": "1.0.0",
  "description": "Template Discord Bot TypeScript",
  "main": "./build/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch ./node_modules ./src/app.ts",
    "clean": "rimraf ./build",
    "build": "npm run clean && tsc --project ./tsconfig.json",
    "start": "node ."
  },
  "keywords": [
    "discord",
    "bot",
    "template",
    "typescript"
  ],
  "author": "Tiago Ryan",
  "license": "MIT",
  "dependencies": {
    "colors": "^1.4.0",
    "discord.js": "^13.6.0",
    "dotenv": "^12.0.3",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^17.0.8",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "16.x"
  }
}
